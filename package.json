    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.9",
    "formik": "^1.0.0-alpha.4",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "http-status-codes": "^1.3.0",
    "lodash-es": "^4.17.7",
    "mathjax3": "^3.0.0-alpha.3",
    "popper.js": "^1.14.1",
    "pouchdb-adapter-http": "^6.4.3",
    "pouchdb-adapter-idb": "^6.4.3",
    "prosemirror-commands": "^1.0.4",
    "prosemirror-dropcursor": "^1.0.0",
    "prosemirror-gapcursor": "^1.0.0",
    "prosemirror-history": "^1.0.0",
    "prosemirror-inputrules": "^1.0.1",
    "prosemirror-keymap": "^1.0.0",
    "prosemirror-markdown": "^1.0.3",
    "prosemirror-model": "^1.0.0",
    "prosemirror-schema-basic": "^1.0.0",
    "prosemirror-schema-list": "^1.0.0",
    "prosemirror-state": "^1.1.0",
    "prosemirror-tables": "^0.7.0",
    "prosemirror-transform": "^1.0.4",
    "prosemirror-view": "^1.0.0",
    "qs": "^6.5.1",
    "raven-js": "^3.24.2",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-modal": "^3.1.11",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^2.0.0-beta.4",
    "rxdb": "^7.6.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "style-loader": "^0.21.0",
    "styled-components": "^3.0.1",
    "ts-loader": "^4.0.0",
    "typeface-charis-sil": "^0.0.44",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "uuid": "^3.2.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.9",
    "webpack-merge": "^4.1.2",
    "yup": "^0.24.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.1",
    "@storybook/addon-links": "^4.0.0-alpha.1",
    "@storybook/addon-notes": "^4.0.0-alpha.1",
    "@storybook/addon-storyshots": "^4.0.0-alpha.1",
    "@storybook/addons": "^4.0.0-alpha.1",
    "@storybook/react": "^4.0.0-alpha.1",
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.1.0",
    "@types/prop-types": "^15.5.2",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__addon-notes": "^3.3.0",
    "@types/storybook__react": "^3.0.6",
    "axios-mock-adapter": "^1.12.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jest-enzyme": "^6.0.0",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.0.4",
    "prettier": "^1.10.2",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.4.0",
    "webpack-dev-server": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      "styled-components",
      "react-hot-loader/babel",
      "transform-class-properties",
      "syntax-dynamic-import"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-intl",
            {
              "messagesDir": "./build/messages/"
            }
          ]
        ]
      },
      "test": {
        "plugins": [
          "dynamic-import-node"
        ]
      }
    }
  },
  "lint-staged": {
    "*.disabled": [
      "tsc --noEmit --jsx react --lib es2017,dom --typeroots 'node_modules/@types,stories/types,src/types/*.ts'"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint",
      "git add"
    ],
    "*.{js}": [
      "prettier --write",
      "tslint",
      "git add"
    ],
    "*.{css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "overrides": [
      {
        "files": "*.css",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "__tests__.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/src/__mocks__/styleMock.ts",
      "^.+\\.(jpg|png|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.ts",
      "^.+\\.xml$": "<rootDir>/src/__mocks__/rawMock.ts"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/*.d.ts"
    ]
  }
}
