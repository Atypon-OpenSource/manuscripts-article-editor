  "scripts": {
    "start": "TS_NODE_FILES=true webpack-dev-server --open --config webpack.dev.ts",
    "build": "TS_NODE_FILES=true NODE_OPTIONS=--max_old_space_size=4096 webpack --config webpack.prod.ts",
    "cypress:open": "cypress open",
    "docker-compose:testcafe": "docker-compose -f testcafe/docker-compose.yml",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:integration": "jest --config=jest.integration.config.js --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --runInBand --testPathIgnorePatterns=Storyshots.test.ts --watch",
    "testcafe": "testcafe",
    "testcafe-live": "testcafe-live",
    "lint": "tslint --project . --config tslint.config.js",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"{src,stories,tests}/**/*.{js,ts,tsx,css}\"",
    "stats": "yarn --silent build --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json dist --port 9000",
    "storybook": "start-storybook --config-dir stories/config --port 6006",
    "build-storybook": "NODE_OPTIONS=--max_old_space_size=4096 build-storybook --config-dir stories/config",
    "build-modernizr": "modernizr --config modernizr.config.json --dest public/modernizr.js",
    "preversion": "npm-run-all --parallel typecheck lint test",
    "version": "ALLOW_MISSING_VARIABLES=1 yarn build"
  },
