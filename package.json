{
  "name": "manuscripts-frontend",
  "version": "0.0.1",
  "repository": "https://gitlab.com/mpapp-private/manuscripts-frontend",
  "author": "",
  "license": "MIT",
  "main": "src",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tsc --noEmit --project tsconfig.json && tslint --project tsconfig.json",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{src,stories,test}/**/*.{js,ts,tsx,css}\"",
    "now": "now",
    "storybook": "start-storybook --config-dir stories/config --port 6006",
    "build-storybook": "build-storybook --config-dir stories/config"
  },
  "dependencies": {
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.3.0",
    "@types/pouchdb-adapter-http": "^6.1.2",
    "@types/pouchdb-adapter-idb": "^6.1.2",
    "@types/prosemirror-commands": "^1.0.0",
    "@types/prosemirror-gapcursor": "^1.0.0",
    "@types/prosemirror-history": "^1.0.0",
    "@types/prosemirror-inputrules": "^1.0.0",
    "@types/prosemirror-keymap": "^1.0.0",
    "@types/prosemirror-markdown": "^1.0.0",
    "@types/prosemirror-model": "^1.1.1",
    "@types/prosemirror-schema-basic": "^1.0.0",
    "@types/prosemirror-schema-list": "^1.0.0",
    "@types/prosemirror-state": "^1.0.0",
    "@types/prosemirror-tables": "^0.1.2",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-modal": "^3.1.1",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/uuid": "^3.4.3",
    "@types/webpack-env": "^1.13.3",
    "@types/yup": "^0.24.0",
    "axios": "^0.18.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "formik": "^0.11.5",
    "html-webpack-plugin": "^2.30.1",
    "http-status-codes": "^1.3.0",
    "lodash": "^4.17.5",
    "pouchdb-adapter-http": "^6.4.3",
    "pouchdb-adapter-idb": "^6.4.3",
    "prosemirror-commands": "^1.0.4",
    "prosemirror-dropcursor": "^1.0.0",
    "prosemirror-gapcursor": "^1.0.0",
    "prosemirror-history": "^1.0.0",
    "prosemirror-inputrules": "^1.0.1",
    "prosemirror-keymap": "^1.0.0",
    "prosemirror-markdown": "^1.0.3",
    "prosemirror-model": "^1.0.0",
    "prosemirror-schema-basic": "^1.0.0",
    "prosemirror-schema-list": "^1.0.0",
    "prosemirror-state": "^1.1.0",
    "prosemirror-tables": "^0.6.2",
    "prosemirror-transform": "^1.0.4",
    "prosemirror-view": "^1.0.0",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rxdb": "7.3.3",
    "rxjs": "^5.5.6",
    "style-loader": "^0.20.1",
    "styled-components": "^3.0.1",
    "ts-loader": "^3.3.1",
    "typeface-charis-sil": "^0.0.44",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "uuid": "^3.2.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1",
    "yup": "^0.24.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addon-notes": "^3.3.11",
    "@storybook/addon-storyshots": "^3.3.11",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.1.0",
    "@types/prop-types": "^15.5.2",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__addon-notes": "^3.3.0",
    "@types/storybook__react": "^3.0.6",
    "axios-mock-adapter": "^1.12.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.0.2",
    "lint-staged": "^6.0.1",
    "prettier": "^1.10.2",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.4.0",
    "webpack-dev-server": "^2.11.1"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-class-properties"
    ]
  },
  "lint-staged": {
    "*.disabled": [
      "tsc --noEmit --jsx react --lib es2017,dom --typeroots 'node_modules/@types,stories/types,src/types/*.ts'"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint",
      "git add"
    ],
    "*.{js}": [
      "prettier --write",
      "tslint",
      "git add"
    ],
    "*.{css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "__tests__.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/src/__mocks__/styleMock.ts",
      "^.+\\.(jpg|png|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.ts"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/*.d.ts"
    ]
  }
}
