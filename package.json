    "@types/prosemirror-state": "^1.0.0",
    "@types/prosemirror-tables": "^0.1.2",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.0.35",
    "@types/react-dnd": "^2.0.36",
    "@types/react-dnd-html5-backend": "^2.1.9",
    "@types/react-dom": "^16.0.3",
    "@types/react-intl": "^2.3.5",
    "@types/react-loadable": "^5.3.3",
    "@types/react-modal": "^3.1.1",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-select": "^1.2.6",
    "@types/uuid": "^3.4.3",
    "@types/webpack-env": "^1.13.5",
    "@types/yup": "^0.24.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "citeproc": "^2.1.196",
    "clean-webpack-plugin": "^0.1.18",
    "codemirror": "^5.36.0",
    "css-loader": "^0.28.10",
