{
  "name": "manuscripts-frontend",
  "version": "0.0.1",
  "repository": "https://gitlab.com/mpapp-private/manuscripts-frontend",
  "author": "",
  "license": "MIT",
  "main": "src",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tslint --project tsconfig.json",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts,tsx,css}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "formik": "^0.10.5",
    "npm": "^5.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.0.1",
    "yup": "^0.24.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.1.0",
    "@types/node": "^9.3.0",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__react": "^3.0.6",
    "@types/webpack-env": "^1.13.3",
    "@types/yup": "^0.24.0",
    "babel-core": "^6.26.0",
    "axios-mock-adapter": "^1.12.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.0.2",
    "lint-staged": "^6.0.1",
    "prettier": "^1.10.2",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.3.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "latest",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|\\.test\\.tsx?$)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
