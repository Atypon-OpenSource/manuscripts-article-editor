{
  "name": "manuscripts-frontend",
  "version": "0.0.1",
  "repository": "https://gitlab.com/mpapp-private/manuscripts-frontend",
  "author": "",
  "license": "MIT",
  "main": "src",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tsc --noEmit --project tsconfig.json && tslint --project tsconfig.json",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{src,stories,test}/**/*.{js,ts,tsx,css}\"",
    "now": "now",
    "storybook": "start-storybook --config-dir stories/config --port 6006",
    "build-storybook": "build-storybook --config-dir stories/config"
  },
  "dependencies": {
    "@types/codemirror": "^0.0.56",
